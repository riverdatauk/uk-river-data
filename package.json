{
  "name": "uk-river-data",
  "version": "0.9.0-dev",
  "description": "Access level, flow and other data for UK rivers and other watercourses.",
  "browser": "./dist/uk-river-data.min.js",
  "exports": "./dist/index.cjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist types && rollup -c && tsc --project tsconfig.types.json",
    "coverage": "rimraf coverage && c8 -r html -r text npm run test:unit",
    "lint": "eslint . --fix && prettier . --write",
    "lint:check": "eslint . && prettier . --check",
    "test:unit": "mocha src/**/*.spec.ts"
  },
  "keywords": ["river flow", "river level", "groundwater", "tides"],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "c8": "^7.13.0",
    "camelcase": "^7.0.1",
    "chai": "^4.3.7",
    "eslint": "^8.41.0",
    "eslint-plugin-mocha": "^10.1.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
